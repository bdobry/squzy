<Alloy>
	<NavigationWindow id="mainNav" platform="ios">
        <Window id="main" title="Training Screen">
            <LeftNavButton>
            	<Button id="menuButton" onClick="goActivity"> back </Button>
            </LeftNavButton>
            
             <ScrollableView>
	        	<View >
	        		<View id="countdownWrap">
	        			<Label id="countdown"></Label>
	        			<Label id="countdownBottom">squizes left</Label>
	        		</View>
		        	<View id="wrap">
		        		<View id="timerBg"></View>
		        		<View id="timer"></View>
		        	</View>
		        	<View>
		        		<Button id="pusher">press and squeeze</Button>
		        	</View>
	        	</View>
	        </ScrollableView>
            
            
        </Window>
    </NavigationWindow>
</Alloy>




